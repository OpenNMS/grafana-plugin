{
  "name": "opennms-grafana-plugin",
  "version": "12.0.0-SNAPSHOT",
  "description": "An OpenNMS Integration for Grafana",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenNMS/grafana-plugin.git"
  },
  "author": {
    "name": "OpenNMS",
    "url": "https://www.opennms.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://opennms.atlassian.net/projects/OPG"
  },
  "homepage": "https://github.com/OpenNMS/grafana-plugin",
  "scripts": {
    "build": "webpack -c ./webpack.config.ts --env production",
    "dev": "webpack -c ./webpack.config.ts --env development",
    "docs": "antora --stacktrace generate local-site.yml",
    "e2e": "playwright test",
    "e2e:update": "playwright test --update-screenshots",
    "lint": "eslint --cache --ignore-pattern './.gitignore' --ignore-pattern 'public/**' --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "npm run watch",
    "test": "jest",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:watch": "jest --watch --onlyChanged",
    "typecheck": "tsc --noEmit",
    "validate-xrefs": "antora --generator @antora/xref-validator local-site.yml",
    "watch": "webpack -w -c ./webpack.config.ts --env development"
  },
  "devDependencies": {
    "@antora/cli": "^3.1.14",
    "@antora/site-generator-default": "^3.1.12",
    "@babel/core": "^7.28.3",
    "@grafana/e2e-selectors": "^12.1.2",
    "@grafana/eslint-config": "^8.2.0",
    "@grafana/plugin-e2e": "^2.2.3",
    "@grafana/tsconfig": "^2.0.1",
    "@opennms/style": "github:OpenNMS/opennms-style#v0.4.0",
    "@playwright/test": "^1.56.1",
    "@stylistic/eslint-plugin-ts": "^2.13.0",
    "@swc/core": "~1.3.90",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/flot": "~0.0.32",
    "@types/grafana": "github:CorpGlory/types-grafana",
    "@types/jest": "^29.5.14",
    "@types/jquery": "^3.5.33",
    "@types/node": "^20.19.23",
    "@types/react-router-dom": "^5.3.3",
    "commander": "^11.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.38.0",
    "eslint-plugin-jsdoc": "^51.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-webpack-plugin": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "fs-extra": "^11.1.0",
    "glob": "^10.4.5",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-axios": "^4.9.0",
    "prettier": "^3.6.2",
    "recursive-copy": "^2.0.13",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "rimraf": "^6.0.1",
    "sass": "^1.93.0",
    "sass-loader": "^16.0.5",
    "specit": "^1.4.4",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.3.1",
    "typescript": "^5.9.3",
    "urijs": "^1.19.11",
    "webpack": "^5.102.1",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2",
    "which": "^4.0.0"
  },
  "engines": {
    "node": ">=20 <23"
  },
  "spec": {
    "specTemplate": "src/rpm/spec.mustache",
    "installDir": "/var/lib/grafana/plugins",
    "requires": [
      "grafana >= 12.0.0"
    ]
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@grafana/data": "^12.2.1",
    "@grafana/i18n": "^12.2.1",
    "@grafana/runtime": "^12.2.1",
    "@grafana/schema": "^12.2.1",
    "@grafana/ui": "^12.2.1",
    "async": "^3.2.3",
    "crypto-js": "^4.2.0",
    "flot": "^0.8.3",
    "flot-axislabels": "https://github.com/j-white/flot-axislabels#master",
    "lodash.clonedeep": "^4.5.0",
    "lodash.escaperegexp": "^4.1.2",
    "opennms": "^2.5.11",
    "parenthesis": "^3.1.8",
    "perfect-scrollbar": "^1.5.5",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "react-router-dom": "^7.9.4",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "packageManager": "npm@10.8.1"
}
