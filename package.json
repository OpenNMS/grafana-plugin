{
  "name": "opennms-helm",
  "version": "9.0.0-SNAPSHOT",
  "description": "An OpenNMS Integration for Grafana",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenNMS/opennms-helm.git"
  },
  "author": "Benjamin Reed",
  "license": "MIT",
  "bugs": {
    "url": "https://issues.opennms.org/projects/HELM/summary"
  },
  "homepage": "https://github.com/OpenNMS/opennms-helm",
  "devDependencies": {
    "@antora/cli": "^3.1.2",
    "@antora/site-generator-default": "^3.1.2",
    "@babel/core": "^7.16.7",
    "@grafana/e2e": "^9.4.3",
    "@grafana/e2e-selectors": "^9.4.3",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@opennms/style": "github:OpenNMS/opennms-style#v0.4.0",
    "@swc/core": "^1.2.144",
    "@swc/helpers": "^0.4.12",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/flot": "0.0.32",
    "@types/glob": "^8.0.0",
    "@types/grafana": "github:CorpGlory/types-grafana",
    "@types/jest": "^29.2.2",
    "@types/jquery": "^3.5.13",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.9",
    "@types/react-router-dom": "^5.3.3",
    "commander": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.0",
    "fs-extra": "^11.1.0",
    "glob": "^8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-mock-axios": "^4.7.0-beta2",
    "prettier": "^2.5.0",
    "recursive-copy": "^2.0.13",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "rimraf": "^4.1.2",
    "sass": "1.56.1",
    "sass-loader": "13.2.0",
    "specit": "^1.4.4",
    "style-loader": "3.3.1",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "urijs": "^1.19.11",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-livereload-plugin": "^3.0.2",
    "which": "^3.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "spec": {
    "specTemplate": "src/rpm/spec.mustache",
    "installDir": "/var/lib/grafana/plugins",
    "requires": [
      "grafana >= 9.3.8"
    ]
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "^9.3.8",
    "@grafana/runtime": "^9.3.8",
    "@grafana/ui": "^9.3.8",
    "async": "^3.2.3",
    "crypto-js": "^4.1.1",
    "flot": "^0.8.3",
    "flot-axislabels": "https://github.com/j-white/flot-axislabels#master",
    "lodash": "^4.17.21",
    "opennms": "^2.5.0",
    "parenthesis": "^3.1.7",
    "perfect-scrollbar": "^1.5.5",
    "q": "^1.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.2.0",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -c ./.config/webpack/webpack.config.ts --env development",
    "docs": "antora --stacktrace generate local-site.yml",
    "e2e": "yarn cypress install && yarn grafana-e2e run",
    "e2e:update": "yarn cypress install && yarn grafana-e2e run --update-screenshots",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "npm run watch",
    "test": "jest",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:watch": "jest --watch --onlyChanged",
    "typecheck": "tsc --noEmit",
    "validate-xrefs": "antora --generator @antora/xref-validator local-site.yml",
    "watch": "webpack -w -c ./.config/webpack/webpack.config.ts --env development"
  }
}
